# Production Environment Configuration
ENVIRONMENT=production

# Supabase Configuration (Production)
SUPABASE_URL=your_prod_supabase_project_url_here
SUPABASE_KEY=your_prod_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_prod_supabase_service_role_key_here

# Claude API Configuration
ANTHROPIC_API_KEY=your_production_claude_api_key_here

# Security Configuration (MUST CHANGE IN PRODUCTION)
SECRET_KEY=your_very_secure_production_secret_key_here

# Production Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Production CORS (RESTRICTIVE - Add your domains)
CORS_ORIGINS=https://yourdomain.com,https://app.yourdomain.com

# Production File Upload
MAX_FILE_SIZE=104857600
UPLOAD_DIR=uploads

# Production Logging
LOG_LEVEL=INFO
